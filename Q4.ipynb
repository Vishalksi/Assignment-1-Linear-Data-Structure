    "\n",
    "\n",
    "a = input(\"enter the string \")\n",
    "index = -1\n",
    "b = \"\"\n",
    "for i in a:\n",
    "    if a.count(i) == 1:\n",
    "        b += i\n",
    "        break\n",
    "print(\"First non-repeating character is\", b)"
